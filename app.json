{
  "expo": {
    "name": "SOLO HOK",
    "slug": "solo-hok",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#222831"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.solohok.app"
    },
    "android": {
      "package": "com.solohok.app",
      "backgroundColor": "#222831"
    },
    "web": {
      "bundler": "metro"
    }
  }
}